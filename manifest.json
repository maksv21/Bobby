{
  "manifest_version": 2,
  "name": "Bobby",
  "version": "1.0",
  "description": "Font viewer",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/48.png"
  },
  "permissions": [
    "fontSettings"
  ],
  "background": {
    "scripts": [
      "scripts/background.js"
    ]
  }
}